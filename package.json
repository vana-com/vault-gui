{
  "name": "vault-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:ci": "next build && next start",
    "lint": "next lint",
    "pretty": "prettier --check",
    "cypress": "cypress open",
